package august_8;

public class MainClass {

	public static void main(String[] args) {
		Ornek1();

	}
	
	private static void Ornek1()
	{
		//enumeration
		//soyut kavramları kategorize etmek için
		LEVEL level = LEVEL.HIGH;
		
		if(level == LEVEL.HIGH)
		{
			System.err.println("Seviye Yüksek");
		}
	}
	
	enum LEVEL
	{
		HIGH,MEDIUM,LOW
	}
}
